<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.olparc"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="19" />
	<uses-permission android:name="android.permission.INTERNET"/>
    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.example.olparc.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        
        <activity
            android:name="com.example.olparc.RegisterActivity"
            android:label="Register" >
        </activity>
         <activity
            android:name="com.example.olparc.About"
            android:label="About Us" >
        </activity>
         <activity
            android:name="com.example.olparc.OnlinePurchase"
            android:label="Online Purchasing" >
        </activity>
         <activity
            android:name="com.example.olparc.Results"
            android:label="Results Checking" >
        </activity>
         <activity
            android:name="com.example.olparc.Contact"
            android:label="Contact Us" >
        </activity>
         <activity
            android:name="com.example.olparc.profilemainpage"
            android:label="Profile" >
        </activity>
    </application>

</manifest>
